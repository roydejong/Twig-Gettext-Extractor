{
    "name": "roydejong/twig-gettext-extractor",
    "type": "application",
    "description": "(Updated for Twig 2.2) The Twig Gettext Extractor is Poedit friendly tool which extracts translations from twig templates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Saša Stamenković",
            "email": "umpirsky@gmail.com"
        }
    ],
    "require": {
        "php":                 ">=5.5.9",
        "twig/twig":           "~2.2",
        "twig/extensions":     "~1.4",
        "symfony/filesystem":  "~3.0",
        "symfony/translation": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit":     "~4.8"
    },
    "autoload": {
        "psr-0": { "Twig\\Gettext": "." }
    },
    "bin": ["twig-gettext-extractor"],
    "config": {
        "bin-dir": "bin"
    }
}
